@model IEnumerable<WebBanHangOnline.Models.EF.Category>

@if (Model != null && Model.Any())
{
<ul class="navbar_menu">
    @foreach (var item in Model)
    {
        if (item.Alias.Equals("trang-chu"))
        {
            item.Alias = "/trang-chu";
        }
        if (item.Alias.Equals("tin-tuc"))
        {
            item.Alias = "/tin-tuc";
        }
        if (item.Alias.Equals("bai-viet"))
        {
            item.Alias = "/bai-viet";
        }
        if (item.Alias.Equals("san-pham"))
        {
            item.Alias = "/san-pham";
        }
        if (item.Alias.Equals("lien-he"))
        {
            item.Alias = "/lien-he";
        }
        if (!string.IsNullOrEmpty(item.Link))
        {
            item.Alias = item.Link;
        }
        <li><a href="@item.Alias">@item.Title</a></li>
    }

</ul>
}